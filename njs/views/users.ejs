<%- include('partials/header.ejs'); -%>

    <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
      <h5 class="py-3" >List of Users</h3>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>UserID</th>
              <th>Name</th>
              <th>Last Login</th>
              <th>TeamID</th>
              <th>Is Admin ?</th>
            </tr>
           </thead>
         <tbody>
         <% rowlist.forEach(function(row) { %>
            <tr>
              <td><%= row.username %></td>
              <td><%= row.name %></td>
              <td><%= row.lastlogin %></td>
              <td><%= row.teamid %></td>
              <td><%= row.admin %></td>
            </tr>
         <% }); %>
         </tbody>
        </table>
      </div>
    </main>

    <!-- These two divs are from the header --!>
  </div>
</div>
<%- include('partials/footer.ejs'); -%>
